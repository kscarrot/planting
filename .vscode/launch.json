  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "ts-debug",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/${relativeFile}",
      //不使用热更新debug需要开启此选项 否则在debug之前需要执行一次 ts-debug-task-watch
      // "preLaunchTask": "ts-debug-task",
      "outFiles": ["${workspaceFolder}/build/**/*.js"]
    }
  ]
}
