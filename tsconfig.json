{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "target": "es5",
    "outDir": "./build",
    "importHelpers": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "removeComments": true,
    "noFallthroughCasesInSwitch": true,
    "lib": ["esnext", "dom"],
    "baseUrl": "src",
    "paths": {
      "@ds/*": ["datastructure/*"]
    }
  },
  "include": ["src"]
}
