{
  "name": "ts-playground",
  "version": "1.0.0",
  "description": "typescript prlayground",
  "private": false,
  "scripts": {
    "build": "rm -rf build && tsc",
    "commit": "git cz",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "lint-staged",
    "test": "jest",
    "cov": "rm -rf coverage && jest --coverage",
    "format": "prettier --write",
    "tslint": "./node_modules/.bin/tslint -p ./"
  },
  "dependencies": {
    "@types/lodash": "^4.14.138",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/jest": "^24.0.18",
    "babel-jest": "^24.9.0",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,json,md}": [
      "prettier --write",
      "yarn tslint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kscarrot/ts-playground"
  },
  "author": "kscarrot",
  "license": "MIT"
}
